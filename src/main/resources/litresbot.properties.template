# Bot token
botToken=

# Proxy settings
useProxy=false
proxyHost=127.0.0.1
proxyPort=9050
# set to http/socks4/socks5
proxyType=socks5

# the host to access Flibusta
flibustaHost=http://flibusta.is

# where we store downloaded book files
flibustaDownloadPath=./flibooks

# how many results on the Search page
searchPage=10
# how many symbols on the Read Book page
readPage=3000